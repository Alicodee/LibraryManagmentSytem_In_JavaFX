<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="rootpane" prefHeight="333.0" prefWidth="582.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.updateMember.updateMemberController">
   <children>
      <VBox prefHeight="332.0" prefWidth="582.0">
         <children>
            <JFXTextField fx:id="id" labelFloat="true" prefHeight="38.0" prefWidth="517.0" promptText="Member ID">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="name" labelFloat="true" layoutX="10.0" layoutY="10.0" prefHeight="41.0" prefWidth="500.0" promptText="Member Name">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="mobile" labelFloat="true" layoutX="10.0" layoutY="10.0" prefHeight="41.0" prefWidth="500.0" promptText="Mobile">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="email" labelFloat="true" layoutX="10.0" layoutY="51.0" prefHeight="41.0" prefWidth="500.0" promptText="Email">
