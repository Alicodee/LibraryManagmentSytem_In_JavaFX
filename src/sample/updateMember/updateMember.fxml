                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox prefHeight="42.0" prefWidth="582.0">
               <children>
                  <JFXButton buttonType="RAISED" onAction="#updatebuttonHandler" prefHeight="60.0" prefWidth="300.0" ripplerFill="#4a3a3a" stylesheets="@../addBooks/button.css" text="Update" textFill="WHITE">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <cursor>
                        <Cursor fx:constant="OPEN_HAND" />
                     </cursor>
                  </JFXButton>
                  <JFXButton buttonType="RAISED" layoutX="10.0" layoutY="10.0" onAction="#cancelbuttonHandler" prefHeight="60.0" prefWidth="300.0" stylesheets="@../addBooks/button.css" text="Cancel" textFill="WHITE">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <cursor>
                        <Cursor fx:constant="OPEN_HAND" />
                     </cursor>
                  </JFXButton>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
